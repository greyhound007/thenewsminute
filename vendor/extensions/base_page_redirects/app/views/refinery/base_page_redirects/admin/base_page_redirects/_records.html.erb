<%= render 'refinery/admin/search_header', :url => refinery.base_page_redirects_admin_base_page_redirects_path %>
<div class='pagination_container'>
  <% if @base_page_redirects.any? %>
    <%= render 'base_page_redirects' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
