<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(telangana) -%>">
  <span class='title'>
    <%= telangana.headline %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.telanganas_telangana_path(telangana),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_telanganas_admin_telangana_path(telangana),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.telanganas_admin_telangana_path(telangana),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => telangana.headline),
        :method => :delete %>
  </span>
</li>
