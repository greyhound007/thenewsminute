<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(andhra) -%>">
  <span class='title'>
    <%= andhra.headline %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.andhras_andhra_path(andhra),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_andhras_admin_andhra_path(andhra),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.andhras_admin_andhra_path(andhra),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => andhra.headline),
        :method => :delete %>
  </span>
</li>
