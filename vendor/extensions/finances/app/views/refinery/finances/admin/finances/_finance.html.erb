<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(finance) -%>">
  <span class='title'>
    <%= finance.Main_headline %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.finances_finance_path(finance),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_finances_admin_finance_path(finance),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.finances_admin_finance_path(finance),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => finance.Main_headline),
        :method => :delete %>
  </span>
</li>
