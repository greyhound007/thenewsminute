<%= render 'refinery/admin/search_header', :url => refinery.headline2s_admin_headline_2s_path %>
<div class='pagination_container'>
  <% if @headline_2s.any? %>
    <%= render 'headline_2s' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
